
@{
    ViewData["Title"] = "TestStudent";
}

<h1>TestStudent</h1>

@{
    if (User.Identity.IsAuthenticated)
    {
        <h2>Authenticated</h2>
        @foreach (var claim in User.Claims)
        {
            <p>@claim.Type: @claim.Value </p>
        }
    }
    else
    {
        <h2>Not Authenticated</h2>
    }
}}